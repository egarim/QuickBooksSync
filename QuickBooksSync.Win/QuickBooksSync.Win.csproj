<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>ExpressApp.ico</ApplicationIcon>
    <Deterministic>false</Deterministic>
    <AssemblyVersion>1.0.*</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Configurations>Debug;Release;EasyTest</Configurations>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Controllers\SyncController.cs" />
    <Compile Remove="Controllers\SyncController.Designer.cs" />
    <Compile Remove="Controllers\SyncControllerWin.cs" />
  </ItemGroup>
  <ItemGroup>
	<Using Include="DevExpress.ExpressApp.ListView" Alias="ListView" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Model.xafml" />
    <None Remove="Images\Logo.svg" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Model.xafml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Images\Logo.svg" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="DevExpress.ExpressApp" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Chart.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.CodeAnalysis" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Dashboards.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.FileAttachment.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Notifications.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Office.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.PivotChart.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.PivotGrid.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.ReportsV2.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Scheduler.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.ScriptRecorder.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Security.Xpo" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.TreeListEditors.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Validation.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Win" Version="22.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Xpo" Version="22.1.5" />
